    "@Magic Move API": {
        "prefix": "@magic-move-api",
        "body": [
            ":::magic-move",
            "```${1:js} [${2:api.js}]",
            "${3:// Basic fetch}",
            "async function fetchUser(id) {",
            "  const response = await fetch(`/api/users/\\${id}`);",
            "  return response.json();",
            "}",
            "```",
            "```${1:js} [${2:api.js}]",
            "${4:// Add error handling}",
            "async function fetchUser(id) {",
            "  try {",
            "    const response = await fetch(`/api/users/\\${id}`);",
            "    if (!response.ok) {",
            "      throw new Error('Failed to fetch user');",
            "    }",
            "    return response.json();",
            "  } catch (error) {",
            "    console.error('Error:', error);",
            "    throw error;",
            "  }",
            "}",
            "```",
            "```${1:js} [${2:api.js}]",
            "${5:// Add caching}",
            "const userCache = new Map();",
            "",
            "async function fetchUser(id) {",
            "  if (userCache.has(id)) {",
            "    return userCache.get(id);",
            "  }",
            "  ",
            "  try {",
            "    const response = await fetch(`/api/users/\\${id}`);",
            "    if (!response.ok) {",
            "      throw new Error('Failed to fetch user');",
            "    }",
            "    const user = await response.json();",
            "    userCache.set(id, user);",
            "    return user;",
            "  } catch (error) {",
            "    console.error('Error:', error);",
            "    throw error;",
            "  }",
            "}",
            "```",
            ":::"
        ],
        "description": "Magic Move showing API function enhancement."
    },

