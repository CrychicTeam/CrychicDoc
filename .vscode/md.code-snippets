{
    // ========== ESSENTIAL SIDEBAR CONFIGURATION SNIPPETS ==========
    
    // Root configuration (creates separate sidebar section)
    "@Root Sidebar": {
        "prefix": "@root",
        "body": [
            "---",
            "root: true",
            "title: ${1:Section Title}",
            "---"
        ],
        "description": "Root sidebar configuration"
    },
    
    "@Root with Layout": {
        "prefix": "@root-layout",
        "body": [
            "---",
            "root: true",
            "title: ${1:Section Title}",
            "layout: doc", 
            "prev: false",
            "next: false",
            "---"
        ],
        "description": "Root with VitePress layout"
    },
    
    // Directory configuration
    "@Directory Config": {
        "prefix": "@dir",
        "body": [
            "---",
            "title: ${1:Directory Title}",
            "---"
        ],
        "description": "Basic directory configuration"
    },
    
    // Document Title Generators
    "@H1": {
        "prefix": "@h1",
        "body": ["# ${1:Title} {#${2:anchor}}"],
        "description": "Generate H1 title with anchor"
    },

    "@H2": {
        "prefix": "@h2",
        "body": ["## ${1:Title} {#${2:anchor}}"],
        "description": "Generate H2 title with anchor"
    },

    "@H3": {
        "prefix": "@h3",
        "body": ["### ${1:Title} {#${2:anchor}}"],
        "description": "Generate H3 title with anchor"
    },

    "@H4": {
        "prefix": "@h4",
        "body": ["#### ${1:Title} {#${2:anchor}}"],
        "description": "Generate H4 title with anchor"
    },

    "@H5": {
        "prefix": "@h5",
        "body": ["##### ${1:Title} {#${2:anchor}}"],
        "description": "Generate H5 title with anchor"
    },

    "@H6": {
        "prefix": "@h6",
        "body": ["###### ${1:Title} {#${2:anchor}}"],
        "description": "Generate H6 title with anchor"
    },
    
    // Individual config generators
    "@Add Title": {
        "prefix": "@title",
        "body": ["title: ${1:Title}"],
        "description": "Add title field"
    },
    
    "@Add Root": {
        "prefix": "@root-field",
        "body": ["root: ${1|true,false|}"],
        "description": "Add root field"
    },
    
    "@Add Collapsed": {
        "prefix": "@collapsed",
        "body": ["collapsed: ${1|true,false|}"],
        "description": "Add collapsed field"
    },
    
    "@Add Hidden": {
