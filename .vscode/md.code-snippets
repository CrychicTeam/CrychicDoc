            "next: false", 
            "---",
            "",
            "# ${1:Section Title}",
            "",
            "$0"
        ],
        "description": "Complete root section template"
    },

    // ========== Basic Utilities ==========
    "@Insert Non-breaking Space": {
        "prefix": "@nbsp",
        "body": ["&nbsp;&nbsp;"],
        "description": "Insert two non-breaking spaces in Markdown"
    },
    "@Frontmatter Title": {
        "prefix": "@title",
