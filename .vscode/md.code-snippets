    // ---------- Pre-defined Style Sets ----------
    "@LiteTree Status Styles": {
        "prefix": "@lite-status-styles",
        "body": [
            "// Status styles",
            "#success=color:green;font-weight:bold;background:#e6ffe6;padding:2px 6px;border-radius:3px;",
