    "@Magic Move Refactor": {
        "prefix": "@magic-move-refactor",
        "body": [
            ":::magic-move",
            "```${1:js} [${2:utils.js}]",
            "${3:// Before refactoring}",
            "function processData(data) {",
            "  let result = [];",
            "  for(let i = 0; i < data.length; i++) {",
            "    if(data[i].active) {",
            "      result.push({",
            "        id: data[i].id,",
            "        name: data[i].name.toUpperCase()",
            "      });",
            "    }",
            "  }",
            "  return result;",
            "}",
            "```",
            "```${1:js} [${2:utils.js}]",
            "${4:// After refactoring}",
            "function processData(data) {",
            "  return data",
            "    .filter(item => item.active)",
            "    .map(item => ({",
            "      id: item.id,",
            "      name: item.name.toUpperCase()",
            "    }));",
            "}",
            "```",
            ":::"
        ],
        "description": "Magic Move demonstrating code refactoring."
    },

